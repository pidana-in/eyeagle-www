---
import MainLayout from "../layouts/MainLayout.astro";
import HeroSection from "../assets/Navbar/HeroSection.png";
import ourTeam from "../assets/Navbar/ourTeam.png";
import desireImage from "../assets/Navbar/desireImage.png";
import HeroSectionComponent from "../components/Home/HeroSectionComponent.astro";
import HealthOrganization from "../components/Home/HealthOrganization.astro";
import {bathroomData,takeCareData} from "../components/constants/home";
import OurTeamSection from "../components/Home/OurTeamSection.astro";
import DesireSection from "../components/Home/DesireSection.astro";
import ScrollSection from "../components/Home/ScrollSection.astro";
import ProductFeature from "../components/Home/ProductFeature.astro";
import ServiceSection from "../components/Home/ServiceSection.astro";
---
<MainLayout>
  <!-- Hero Section -->
   <HeroSectionComponent heroImage={HeroSection}/>
  <!-- Statistics Section -->
  <HealthOrganization/>
  <!-- Scroll section -->
  <ScrollSection/>
  <!-- Product Features Section -->
   <ProductFeature bathroomData={bathroomData}/>
  <!-- Team Section -->
  <OurTeamSection item={ourTeam}/>
  <!-- Services Section -->
   <ServiceSection takeCareData={takeCareData}/>
  <!-- Desire Section -->
  <DesireSection item={desireImage}/>
</MainLayout>

